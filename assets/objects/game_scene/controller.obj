        '%onPointDown.1':
          '''{(GameObject* self)
            Project::active_project()->enqueueKeyEvent({KeyEventType::KEY_UP, KeyEventState::STATE_PRESSED});
          '''
      nam: 'up'
      pos: [0, 60, 0]
