Obj:
  fnc:
    '%onInit':
      '''{(NodeLoadable* loadable)
        auto self = loadable->cast<GameObject>();
        auto vss = self->getTrait<VATStateSelector>("vertex_animation_texture_state");

        auto b = vss->select("b", 3);
        b->setPos(0.1);
      '''

  vie:
    ImageView:
      tex: 'textures/game_scene/snake.face.happy.png'
      siz: [100, 0]

  trt:
    - VATStateSelector:
      stt:
        - VATStateItem:
          nam:'a'
          vst:
            VATState:
              tsc: 0.2
              vat:
                VAT:
                  tex:
                    - 'vertex_animation_textures/wobble.vat.pos.png'
                  scl: 0.2
                  fcn: 13
        - VATStateItem:
          nam: 'b'
          vst:
            VATState:
              tsc: 0.2
              vat:
                VAT:
                  tex:
                    - 'vertex_animation_textures/wobble.vat.pos.png'
                  ofs: [3, 2, 0]
                  scl: 0.2
                  fcn: 13
