                      rot: [0, 0, 15]
                      img:
                        Image:
                          tex: './sprites/ko.png'
                          siz: {'!relh.pair':[0, 74]}
                          reg: [268, 317, 123, 74]
                    - Cell:
                      frm: 5
                      pos: [30, -17, 0]
                      img:
                        Image:
                          tex: './sprites/ko.png'
                          siz: {'!relh.pair':[0, 76]}
                          reg: [19, 554, 150, 76]
                    - Cell:
                      frm: 5
                      pos: [32, -15, 0]
                      img:
                        Image:
                          tex: './sprites/ko.png'
                          siz: {'!relh.pair':[0, 75]}
                          reg: [20, 476, 148, 75]
                    - Cell:
                      frm: 45
                      pos: [34, -17, 0]
                      img:
                        Image:
                          tex: './sprites/ko.png'
                          siz: {'!relh.pair':[0, 76]}
                          reg: [19, 554, 150, 76]
      acl:
        - ActionItem:
          nam: '%onInit'
