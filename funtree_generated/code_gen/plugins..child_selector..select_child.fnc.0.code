auto self = params[0].as<GameComponent*>();  auto msg_params = params[4].as<vector<Var>>();  

auto name = msg_params[0].as<string>();

for (int ci=0; ci<self->getChildCount(); ++ci) {
  auto chl = self->getChild(ci);
  unlikely (chl == null)
    continue;

  chl->is_drawable_self (chl->name() == name);
  chl->is_updatable_self(chl->name() == name);
}
return null_var;
